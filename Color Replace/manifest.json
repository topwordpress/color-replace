{
    "manifest_version": 3,
    "name": "Color Replacer",
    "version": "1.0",
    "description": "Replace specific colors on websites with custom colors",
    "permissions": [
        "activeTab", "storage"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}
